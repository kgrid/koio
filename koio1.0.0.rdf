<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.knowledgegrid.org/koront#"
     xml:base="http://www.knowledgegrid.org/koront"
     xmlns:koio1="http://www.kgrid.org/koio#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:koio="http://www.knowledgegrid.org/koio#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="http://www.kgrid.org/koio">
        <owl:versionIRI rdf:resource="http://www.kgrid.org/koio/1.0.0/koio.rdf"/>
        <koio:definition>The Knowledge Object Implementation Ontology (KOIO) is an ontology is a formal, standardized representational framekwork spanning what exists and is implemented in the form of Knowledge Objects, as they are defined in KOIO and exist within the Knowledge Grid platform.</koio:definition>
        <dc:title>Knowledge Object Implementation Ontology (KOIO) version 1.0.0</dc:title>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://www.kgrid.org/koio#definition -->

    <owl:AnnotationProperty rdf:about="http://www.kgrid.org/koio#definition">
        <koio1:definition>definition, def., an information artifact that annotates any Thing with a definition of the Thing</koio1:definition>
        <rdfs:label xml:lang="en">definition</rdfs:label>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#type -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#type"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.kgrid.org/koio#hasDeploymentInstructionPart -->

    <owl:ObjectProperty rdf:about="http://www.kgrid.org/koio#hasDeploymentInstructionPart">
        <rdfs:subPropertyOf rdf:resource="http://www.kgrid.org/koio#hasPart"/>
        <owl:inverseOf rdf:resource="http://www.kgrid.org/koio#isDeploymentInstructionPartOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.kgrid.org/koio#Implementation"/>
        <rdfs:range rdf:resource="http://www.kgrid.org/koio#DeploymentInstruction"/>
        <koio1:definition>hasDeploymentInstruction, def., a relation that holds between a whole KnowledgeObject and its DeploymentInstruction parts</koio1:definition>
        <rdfs:isDefinedBy rdf:resource="http://kgrid.org/koio/1.0.0/"/>
        <rdfs:label>hasDeploymentInstruction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.kgrid.org/koio#hasImplementationPart -->

    <owl:ObjectProperty rdf:about="http://www.kgrid.org/koio#hasImplementationPart">
        <rdfs:subPropertyOf rdf:resource="http://www.kgrid.org/koio#hasPart"/>
        <owl:inverseOf rdf:resource="http://www.kgrid.org/koio#isImplementationPartOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.kgrid.org/koio#KnowledgeObject"/>
        <rdfs:range rdf:resource="http://www.kgrid.org/koio#Implementation"/>
        <koio1:definition>hasImplementation, def., a relation that holds between a whole KnowledgeObject and its Implementation part(s)</koio1:definition>
        <rdfs:isDefinedBy rdf:resource="http://kgrid.org/koio/1.0.0/"/>
        <rdfs:label>hasImplementation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.kgrid.org/koio#hasPart -->

    <owl:ObjectProperty rdf:about="http://www.kgrid.org/koio#hasPart">
        <owl:inverseOf rdf:resource="http://www.kgrid.org/koio#isPartOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <koio1:definition>def = a core relation that holds between a whole and its part

BFO</koio1:definition>
        <rdfs:isDefinedBy rdf:resource="http://purl.obolibrary.org/obo/bfo.owl"/>
        <rdfs:label xml:lang="en">hasPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.kgrid.org/koio#hasPayloadPart -->

    <owl:ObjectProperty rdf:about="http://www.kgrid.org/koio#hasPayloadPart">
        <rdfs:subPropertyOf rdf:resource="http://www.kgrid.org/koio#hasPart"/>
        <owl:inverseOf rdf:resource="http://www.kgrid.org/koio#isPayloadPartOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.kgrid.org/koio#Implementation"/>
        <rdfs:range rdf:resource="http://www.kgrid.org/koio#Payload"/>
        <koio1:definition>hasPayload, def., a relation that holds between a whole KnowledgeObject and its Payload part(s)</koio1:definition>
        <rdfs:isDefinedBy rdf:resource="http://kgrid.org/koio/1.0.0/"/>
        <rdfs:label>hasPayload</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.kgrid.org/koio#hasServiceSpecificationPart -->

    <owl:ObjectProperty rdf:about="http://www.kgrid.org/koio#hasServiceSpecificationPart">
        <rdfs:subPropertyOf rdf:resource="http://www.kgrid.org/koio#hasPart"/>
        <owl:inverseOf rdf:resource="http://www.kgrid.org/koio#isServiceSpecificationPartOf"/>
        <rdfs:domain rdf:resource="http://www.kgrid.org/koio#Implementation"/>
        <rdfs:range rdf:resource="http://www.kgrid.org/koio#ServiceSpecification"/>
        <koio1:definition>hasServiceSpecification, def., a relation that holds between a whole KnowledgeObject and its ServiceSpecification part</koio1:definition>
        <rdfs:isDefinedBy rdf:resource="http://kgrid.org/koio/1.0.0/"/>
        <rdfs:label>hasServiceSpecification</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.kgrid.org/koio#isDeploymentInstructionPartOf -->

    <owl:ObjectProperty rdf:about="http://www.kgrid.org/koio#isDeploymentInstructionPartOf">
        <rdfs:subPropertyOf rdf:resource="http://www.kgrid.org/koio#isPartOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.kgrid.org/koio#DeploymentInstruction"/>
        <rdfs:range rdf:resource="http://www.kgrid.org/koio#Implementation"/>
        <koio1:definition>isDeploymentInstruction, def., a core relation that holds between an DeploymentInstruction that is part of a Knowledge Object and its whole</koio1:definition>
        <rdfs:isDefinedBy rdf:resource="http://kgrid.org/koio/1.0.0/"/>
        <rdfs:label>isDeploymentInstructionPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.kgrid.org/koio#isImplementationPartOf -->

    <owl:ObjectProperty rdf:about="http://www.kgrid.org/koio#isImplementationPartOf">
        <rdfs:subPropertyOf rdf:resource="http://www.kgrid.org/koio#isPartOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.kgrid.org/koio#Implementation"/>
        <rdfs:range rdf:resource="http://www.kgrid.org/koio#KnowledgeObject"/>
        <koio1:definition>isImplementation, def., a core relation that holds between an Implementation that is part of a Knowledge Object and its whole</koio1:definition>
        <rdfs:isDefinedBy rdf:resource="http://kgrid.org/koio/1.0.0/"/>
        <rdfs:label>isImplementationPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.kgrid.org/koio#isPartOf -->

    <owl:ObjectProperty rdf:about="http://www.kgrid.org/koio#isPartOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <koio1:definition>isPartOf, def., a core relation that holds between a part and its whole

BFO</koio1:definition>
        <rdfs:isDefinedBy rdf:resource="http://purl.obolibrary.org/obo/bfo.owl"/>
        <rdfs:label xml:lang="en">isPartOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.kgrid.org/koio#isPayloadPartOf -->

    <owl:ObjectProperty rdf:about="http://www.kgrid.org/koio#isPayloadPartOf">
        <rdfs:subPropertyOf rdf:resource="http://www.kgrid.org/koio#isPartOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.kgrid.org/koio#Payload"/>
        <rdfs:range rdf:resource="http://www.kgrid.org/koio#Implementation"/>
        <koio1:definition>isPayload, def., a core relation that holds between an Payload that is part of a Knowledge Object and its whole</koio1:definition>
        <rdfs:isDefinedBy rdf:resource="http://kgrid.org/koio/1.0.0/"/>
        <rdfs:label>isPayloadPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.kgrid.org/koio#isServiceSpecificationPartOf -->

    <owl:ObjectProperty rdf:about="http://www.kgrid.org/koio#isServiceSpecificationPartOf">
        <rdfs:subPropertyOf rdf:resource="http://www.kgrid.org/koio#isPartOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.kgrid.org/koio#ServiceSpecification"/>
        <rdfs:range rdf:resource="http://www.kgrid.org/koio#Implementation"/>
        <koio1:definition>isServiceSpecification, def., a core relation that holds between a ServiceSpecification that is part of a Knowledge Object and its whole</koio1:definition>
        <rdfs:isDefinedBy rdf:resource="http://kgrid.org/koio/1.0.0/"/>
        <rdfs:label>isServiceSpecificationPart</rdfs:label>
    </owl:ObjectProperty>
    


